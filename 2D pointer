#include <stdio.h>
#include<string.h>
#include<ctype.h>

int main() {
    
    int arr[2][3]=
    {
        {1,24,3},
        {4,5,6}
    };
    int N=2;
    int M=3;
    
    for(int i=0;i<2;i++){
        for(int j=0;j<3;j++){
            printf("%d ",*(*(arr+i)+j));
        }
        printf("\n");
    }
    
    return 0;
}
